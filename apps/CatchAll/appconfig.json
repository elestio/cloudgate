{
  "env": "PROD",
  "version": "1.0.0",
  "title": "Catch All App",
  "description": "This is a sample app that catch all domains not configured",
  "domains": ["*"], 
  "publicFolder": "./public",
  "TypeAPI": "LOCAL",
  "TypeFS": "LOCAL",
  "redirect404toIndex": true,
  "apiEndpoints": {
    "/tests/simple" : {
      "src": "./API/tests/",
      "handler": "simple.handler"
    },
    "/tests/full" : {
      "src" : "./API/tests/",
      "handler": "full.handler"
    },
    "/wildcardtest/*" : {
      "src" : "./API/tests/",
      "handler": "full.handler"
    }
  },
  "websocketEndpoints": {
    "/echo" : {
      "src" : "./API/websocket/",
      "open": "Echo.open",
      "message": "Echo.message",
      "close": "Echo.close"
    },
    "/chat" : {
      "src" : "./API/websocket/",
      "open": "Chat.open",
      "message": "Chat.message",
      "close": "Chat.close"
    }
  }
}
