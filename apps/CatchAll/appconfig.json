{
  "env": "PROD",
  "version": "1.0.0",
  "title": "Catch All App",
  "description": "This is a sample app that catch all domains not configured",
  "domains": ["*"], 
  "publicFolder": "./public",
  "TypeAPI": "LOCAL",
  "TypeFS": "LOCAL",
  "redirect404toIndex": false,
  "HSTS": false,
  "maxRequestsPerMinutePerIP": -1,
  "banDurationSeconds": 60,
  "whitelistedIPs":["127.0.0.1", "1.2.3.4"],
  "CORS": {
      "access-control-allow-origin": "*"
  },
  "apiEndpoints": {
    "/tests/simple" : {
      "src": "./API/tests/",
      "handler": "simple.handler"
    },
    "/tests/full" : {
      "src" : "./API/tests/",
      "handler": "full.handler"
    },
    "/tests/full-ratelimited" : {
      "src" : "./API/tests/",
      "handler": "full.handler",
      "maxRequestsPerMinutePerIP": 50
    },
    "/tests/add" : {
      "src": "./API/tests/",
      "handler": "add.handler"
    },
    "/wildcardtest/*" : {
      "src" : "./API/tests/",
      "handler": "full.handler"
    }
  },
  "websocketEndpoints": {
    "/echo" : {
      "src" : "./API/websocket/",
      "upgrade": "Echo.upgrade",
      "open": "Echo.open",
      "message": "Echo.message",
      "close": "Echo.close"
    },
    "/chat" : {
      "src" : "./API/websocket/",
      "open": "Chat.open",
      "message": "Chat.message",
      "close": "Chat.close"
    }
  },
  "rewritings": {
        "/Home": "/index.html",
        "/Yeah/%slug%": "index.html"
    },
    "redirects": {
        "/redirect1": "index.html",
        "/redirect2": "https://google.com/?param=1"
    }
}
