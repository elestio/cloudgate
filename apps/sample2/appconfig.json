{
  "env": "PROD",
  "version": "1.0.0",
  "title": "Sample App 1",
  "description": "This is a sample app",
  "domains": ["192.168.1.19"], 
  "publicFolder": "./public/",
  "db": {
      "MYSQL": {
        "dbName": "MyApp1",
        "dbUser": "myDBUserNameHere",
        "dbPassword": "mysqlSecretPasswordHere",
        "loadFrom": "./DB/MYSQL/dump.sql",
        "backupTo": "./DB/MYSQL/backup.sql"
      },
      "REDIS": {
        "host": "127.0.0.1",
        "port": 6379,
        "redisDB": 0,
        "password": "myRedisSecretPasswordHere",
        "loadFrom": "./DB/REDIS/dump.redis",
        "backupTo": "./DB/REDIS/backup.redis"
      }
  },
  "apiRuntime": "nodejs12",
  "apiEndpoints": {
    "/tests/simple" : {
      "src": "./API/tests/",
      "handler": "simple.handler",
      "timeout": 15, "memory": 256, "cpu": 2, "apiRuntime": "nodejs12"
    },
    "/tests/full" : {
      "src" : "./API/tests/",
      "handler": "full.handler"
    }
  }
}